<?xml version="1.0" encoding="UTF-8"?>
<configuration>
    <system.webServer>
        <httpProtocol>
            <customHeaders>
                <remove name="Access-Control-Allow-Origin" />
                <add name="Access-Control-Allow-Origin" value="http://localhost/" />
            </customHeaders>
        </httpProtocol>
        <defaultDocument>
            <files>
                <add value="Forbidden.htm" />
            </files>
        </defaultDocument>
    </system.webServer>
</configuration>
