@{
    ViewBag.Title = "Licensing Requests";
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";

    var LicenseRequestID = 0;
    var showViewOnLoad = false;
    if (ViewBag.LicenseRequestID != null)
    {
        LicenseRequestID = ViewBag.LicenseRequestID;
        showViewOnLoad = true;
    }
}

<script id="customMessageTemplate" type="text/html">
    <span class="validationError nowrap" data-bind='validationMessage: field'></span>
</script>
@section scripts
{
    <script src="~/Scripts/Application/Model/LicensingRequestModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/LicensingRequestViewModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/WorkflowTaskExecution.js"></script>
    <script>
        $(document).ready(function () {
            $("#CellularNo").kendoMaskedTextBox({
                mask: "(000)000-000-0000"
            });
        });
    </script>
}

@section ko_apply
{
    var showOnLoad = false;
    if ("@showViewOnLoad" == "True")
    {
         showOnLoad = true;
    }
    var viewModel = new IPMSROOT.LicensingRequestViewModel("@LicenseRequestID", showOnLoad);
    ko.applyBindingsWithValidation(viewModel, $("#LicensingRequest")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }});
}

<!-- BEGIN CONTENT -->
<div class="row">
    <div class="col-md-12">
        <h3 class="page-title">Licensing Request</h3>
        <ul class="page-breadcrumb breadcrumb"></ul>
    </div>
</div>

<div class="row"> 
    <div class="portlet" id="divFormWizardTabNavigation">
       <div class="portlet-body form">
         <form action="#" class="form-horizontal" id="submit_form" data-bind="with: licensingrequestModel">
              <div class="form-wizard" data-bind='validationOptions: { messageTemplate: "customMessageTemplate" }' id="LicensingRequest">
                  <div class="form-body">
                            <ul class="nav nav-pills nav-justified steps" id="ulTabs">
                                <li>
                                    <a data-toggle="tab" class="step" data-bind="click: $root.GotoTab1" style="cursor: pointer">
                                        <span class="number">1
                                        </span>
                                        <span class="desc">
                                            <i class="fa fa-check"></i>Registration Details
                                        </span>
                                    </a>
                                </li>
                                <li>
                                    <a data-toggle="tab" class="step" data-bind="click: $root.GotoTab2" style="cursor: pointer">
                                        <span class="number">2
                                        </span>
                                        <span class="desc">
                                            <i class="fa fa-check"></i>Documents Upload 
                                        </span>
                                    </a>
                                </li>
                                <li>
                                    <a data-toggle="tab" class="step" data-bind="click: $root.GotoTab3" style="cursor: pointer">
                                        <span class="number">3
                                        </span>
                                        <span class="desc">
                                            <i class="fa fa-check"></i>Declaration & Submit
                                        </span>
                                    </a>
                                </li>
                            </ul>
                            <div id="bar" class="progress progress-striped" role="progressbar">
                                <div class="progress-bar progress-bar-success">
                                </div>
                            </div>
                            <div class="tab-content">

                                <!--Tab1 Starts-->
                                <div class="tab-pane active" id="tab1" data-bind="visible: viewModeForTabs() == 'registration', template: { name: 'registration-template' }">
                                </div>
                                <!--Tab1 ends here-->
                                <!--Tab2 Starts here-->
                                <div class="tab-pane active" id="tab2" data-bind="visible: viewModeForTabs() == 'documentupload', template: { name: 'documentupload-template' }">
                                </div>
                                <!--Tab2 ends here-->

                                <!--Tab2 Starts here-->
                                <div class="tab-pane active" id="tab3" data-bind="visible: viewModeForTabs() == 'declaretion', template: { name: 'declaretion-template' }">
                                </div>
                                <!--Tab2 ends here-->

                            </div>
                        </div>
                  <div class="form-actions fluid">
                            <div class="row">
                                <div class="col-md-12">
                                    <div class="btns-group" data-bind="loadingWhen: $root.viewModelHelper.isLoading">
                                        <a href="javascript:;" class="btn default button-previous" id="btnGoBack" data-bind="click: $root.GoToPrevTab, visible: $root.isGoBackVisible"><i class="m-icon-swapleft"></i> Back</a>
                                        <a href="#" class="btn default button-next" data-bind="click: $root.ValidateForm, visible: $root.isSaveVisible">Next <i class="m-icon-swapright"></i></a>
                                        <a href="#" class="btn default button-next" data-bind="click: $root.ValidateForm, visible: $root.isUpdateVisible">Next <i class="m-icon-swapright"></i></a>
                                        <a href="javascript:;" class="btn green button-submit" data-bind="click: $root.SaveLicensingRequest, visible: $root.isSubmitVisible">Submit <i class="m-icon-swapright m-icon-white"></i></a>
                                        <button type="submit" class="btn blue" data-bind="click: $root.ResetLicensingRequest, visible: $root.IsReset">Reset</button>
                                        <button type="submit" class="btn red button-submit" data-bind="click: $root.CancelLicensingRequest">Cancel</button>
                                    </div>
                                </div>
                            </div>
                    </div>
                  <div class="form-actions fluid" data-bind="with: licensingrequestModel">
                            @{Html.RenderPartial(Url.Content("~/Views/Shared/PendingTaskView.cshtml"));}
                  </div>
              </div>
         </form>
       </div>
    </div>   
</div>

<!-- LicensingRequest form for generate LicensingRequest form starts here-->

