<style type="text/css">
P.breakhere {page-break-before: always}
    .txt {
        font-size: 13px; /*font-weight:bold;*/
        margin: 0;
        padding: 3px 5px;
        color: #999;
        background: #d5d5d5;
        display: inline-block;
        margin-left: 45%;
    }

    .txtline {
        position: relative;
        z-index: 2;
    }

        .txtline:after {
            content: "";
            position: absolute;
            top: 50%;
            left: 0;
            right: 0;
            border-top: solid 1px #b1b1b1;
            z-index: -1;
        }

    div#pop-up {
        display: none;
        position: absolute;
        width: 500px;
        padding: 3px;
        background: #eeeeee;
        color: #000000;
        border: 1px solid #1a1a1a;
        font-size: 90%;
        z-index: 999;
        border-radius: 4px;
    }

    .legend-new {
        background: red;
    }

    .legend-resubmit {
        background: orange;
    }

    .legend-reject {
        background: #8b0000;
    }

    .legend-approved {
        background: green;
    }

    .legend-na {
        background: grey;
    }

    .legend-inprogress {
        background: yellow;
    }

    .legend-icon {
        width: 16px;
        height: 16px;
        border-radius: 3px;
        border: 1px solid #666;
    }

    .left-span {
        padding-left: 18px;
    }

    .main-title1, .hide-print, .hide-print1 {
        display: none;
    }
</style>
