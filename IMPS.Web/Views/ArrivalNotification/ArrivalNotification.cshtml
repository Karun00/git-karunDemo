<script type="text/javascript">
    function alertf() {
        var moveDown = -200;
        var moveRight = 35;
        $('a#trigger').hover(function (e) {
            $('div#pop-up').show();

        }, function () {
            $('div#pop-up').hide();
        });

        $('a#trigger').mousemove(function (e) {
            $("div#pop-up").css('top', e.pageY + moveDown).css('right', moveRight);
        });
    }
</script>

}
@section ko_apply
{

    var showOnLoad = false;
    if ("@showViewOnLoad" == "True")
    {
         showOnLoad = true;
    }
    var viewModel = new IPMSROOT.ArrivalNotificationViewModel("@vcn","@WorkflowInstanceId", showOnLoad);	   	
	ko.applyBindingsWithValidation(viewModel, $("#divArrivalNotifications")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }}   );
    

}


