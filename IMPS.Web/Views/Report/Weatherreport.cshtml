@using MvcReportViewer;
@{
    Layout="~/Views/Shared/_IPMSLayout.cshtml";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title></title>
</head>
<body>
    <div>
    @Html.MvcReportViewer(
   "/IPMS.Reports/MatrixWeatherrep",
    new { FromDate = DateTime.Today.ToString("yyyy-MM-dd"), ToDate = DateTime.Today.ToString("yyyy-MM-dd"), Port=ViewBag.PortName },
    new { Height = "550px", Width = "100%",scrolling="no", style = "border: none"})
    </div>
</body>
</html>





