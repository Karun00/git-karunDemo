        <script type="text/html" id="List-template">
<div data-bind="loadingWhen: $root.viewModelHelper.isLoading">
            <div id="tableheader">
               <a class="button gbtngreen" data-bind="click: $root.adddeploymentplan">Add New <i class="fa fa-plus"></i></a>
               
            </div>
            <div data-bind="kendoGrid: {
            data: DeploymentPlanList,
            groupable: false,
            sortable: true,
            scrollable: true,
            filterable: { extra: false, operators: { string: { startswith: 'Starts with', eq: 'Is equal to', neq: 'Is not equal to' } } },
            rowTemplate: 'rowTmpl',
            pageable: { pageSize: 5, pageSizes: 5 },
            useKOTemplates: true, columns: [{ field: 'DateNameSort', title: 'Financial Year', width: '80', filterable: true }
                       ,{ field: 'PortNameSort', title: 'Port Name', width: '80', filterable: true }
                       , { field: 'DescriptionSort', title: 'Description', width: '80', filterable: true },
                       { title: 'Status', filterable: false }, { title: 'Actions', filterable: false }]

            }">
            </div>
            <script id="rowTmpl" type="text/html">
                <tr>
                    <td><span data-bind="text: DateName"></span></td>
                      <td>
                        <div class="nowrap"><span data-bind="text: PortName"></span></div>
                    </td>
                    <td>
                        <div class="nowrap"><span data-bind="text: Description"></span></div>
                    </td>
                  
                    <td>
                        <div class="nowrap">
                            <span data-bind="text: Statust"></span>
                        </div>
                    </td>
                    <td>
                        <div class="nowrap">
                        
                            <a href="#" data-bind="click: $parent.editdeploymentplan, visible: EditPending " title="Edit"><i class="fa fa-edit"></i></a>
                           
                            <a href="#" data-bind="click: $parent.viewdeploymentplan" title="View"><i class="fa fa-eye"></i></a>  
                            
                        </div>
                    </td>
                </tr>
            </script>
</div>
