@model IPMS.Domain.ValueObjects.PrivilegeVO

@{
    ViewBag.Title = "DeploymentPlan";
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";
}
@section scripts
{
    <script src="~/Scripts/Application/Model/DeploymentPlanModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/DeploymentPlanViewModel.js"></script>
 <script src="~/Scripts/Lib/bootbox.min.js"></script>
}
<style type="text/css">
    .legend-icon {
        width: 12px;
        height: 12px;
        border-radius: 3px;
        border: 1px solid #666;
    }

        .legend-icon span {
            margin-left: 15px;
            margin-top: -4px !important;
            display: block;
        }

    .td-width {
        width: 230px;
    }
</style>
@section ko_apply
{

    var viewModel = new IPMSROOT.DeploymentPlanViewModel();
  
ko.applyBindingsWithValidation(viewModel, $("#DeploymentPlan")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }});
}
