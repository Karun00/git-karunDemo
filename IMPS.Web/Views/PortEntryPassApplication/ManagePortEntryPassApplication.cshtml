@{
    ViewBag.Title = "ManagePortEntryPassApplication";
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";


    var LicenseRequestID = 0;
    var showViewOnLoad = false;
    if (ViewBag.LicenseRequestID != null)
    {
        LicenseRequestID = ViewBag.LicenseRequestID;
        showViewOnLoad = true;
    }
}
@section scripts
{
<script src="~/Scripts/Application/Model/PortEntryPassApplicationModel.js"></script>
<script src="~/Scripts/Application/ViewModel/PortEntryPassApplicationViewModel.js"></script>
<script src="~/Scripts/Application/ViewModel/WorkflowTaskExecution.js"></script>


}
@section ko_apply
{
    var showOnLoad = 1;
    if ("@showViewOnLoad" == "True")
    {
         showOnLoad = 1;
    }


    var viewModel = new IPMSROOT.PortEntryPassApplicationViewModel("@LicenseRequestID", showOnLoad);
    ko.applyBindingsWithValidation(viewModel, $("#PortEntryPassApplication")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }});
}
<style>
    .alert {
        padding: 4px !important;
