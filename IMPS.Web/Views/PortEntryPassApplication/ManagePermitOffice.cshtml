
@model IPMS.Domain.ValueObjects.PermitRequestVerifyedDetailVO
@{
    ViewBag.Title = "ManagePortEntryPassApplicationList";
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";

    var PermitRequestID = "";
    var showViewOnLoad = false;
    if (!string.IsNullOrEmpty(ViewBag.PermitRequestID))
    {
        PermitRequestID = ViewBag.PermitRequestID;
        showViewOnLoad = true;
    }

}


<style type="text/css">
    .txt {
        font-size: 13px; /*font-weight:bold;*/
        margin: 0;
        padding: 3px 5px;
        color: #999;
        background: #d5d5d5;
        display: inline-block;
        margin-left: 45%;
    }

    .txtline {
        position: relative;
        z-index: 2;
    }

        .txtline:after {
            content: "";
            position: absolute;
            top: 50%;
            left: 0;
            right: 0;
            border-top: solid 1px #b1b1b1;
            z-index: -1;
        }
</style>
<script id="customMessageTemplate" type="text/html">
    <span class="validationError" data-bind='validationMessage: field'></span>
</script>

@section scripts{
@*    <script src="~/Scripts/Application/Model/portentrypassapplicationModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/InternalEmployeePermitViewModel.js"></script>*@
    <script src="~/Scripts/Application/ViewModel/PortEntryPassApplicationViewModel.js"></script>
    <script src="~/Scripts/Application/Model/PortEntryPassApplicationModel.js"></script>
}
@section ko_apply
{

    var showOnLoad = 5;
    if ("@showViewOnLoad" == "True")
    {
         showOnLoad = 5;
    }
    var viewModel = new IPMSROOT.PortEntryPassApplicationViewModel("@PermitRequestID", showOnLoad);	   	
	ko.applyBindingsWithValidation(viewModel, $("#divArrivalNotifications")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }}   );
    

}
<div class="row">
    <div class="col-md-12">
