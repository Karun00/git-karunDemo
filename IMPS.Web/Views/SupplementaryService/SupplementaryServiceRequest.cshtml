@model IPMS.Domain.ValueObjects.PrivilegeVO
@{
    ViewBag.Title = "SupplementaryServiceRequest";
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";

    var SuppServiceRequestID = "0";
    var showViewOnLoad = false;
    if (ViewBag.SuppServiceRequestID != null)
    {
        SuppServiceRequestID = ViewBag.SuppServiceRequestID;
        showViewOnLoad = true;
    }    
}

@section scripts
{
    <script src="~/Scripts/Application/Model/SupplementaryServiceRequestModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/SupplementaryServiceRequestViewModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/WorkflowTaskExecution.js"></script>
}

@section ko_apply
{

    var showOnLoad = false;
    if ("@showViewOnLoad" == "True")
    {
         showOnLoad = true;
    }

var viewModel = new IPMSROOT.SupplementaryServiceRequestViewModel("@SuppServiceRequestID", showOnLoad);
     ko.applyBindingsWithValidation(viewModel, $("#SupplementaryServiceRequest")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }});
}

