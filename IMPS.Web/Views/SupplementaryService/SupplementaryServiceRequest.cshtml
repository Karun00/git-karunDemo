<div id="SupplementaryServiceRequest">

    <div data-bind="visible: viewMode() == 'List', template: { name: 'SupplementaryServiceRequestList-template' }"></div>
    <div data-bind="visible: viewMode() == 'Form', template: { name: 'SupplementaryServiceRequestForm-template' }"></div>
    <div data-bind="visible: viewMode() == 'PopUp', template: { name: 'PopUp-template' }"></div>

</div>

<!-- Supplementary Service Request List starts here-->
<script type="text/html" id="SupplementaryServiceRequestList-template">

    <div>
        <div class="portlet box advbg">
            <div class="portlet-title">
                <div class="caption">
                    <i class="fa fa-reorder"></i>Advance Search
                </div>
                <div class="tools">
                    <a href="javascript:;" class="collapse"></a>
                </div>
            </div>
            <div class="portlet-body" data-bind="with: suppServiceRequestModelGrid">
