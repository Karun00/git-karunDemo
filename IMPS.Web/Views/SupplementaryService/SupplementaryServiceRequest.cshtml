    rowTemplate: 'rowTmpl',
    pageable: { pageSize: 5, pageSizes: 5 },
    useKOTemplates: true,
    columns: [{ field: 'VCNSort', title: 'VCN', filterable: true },
              { field: 'VesselNameSort', title: 'Vessel Name', filterable: true },
             { field: 'ServiceTypeSort', title: 'Service Type', filterable: true },
             { field: 'RequestDtAndTimeSort', title: 'Request Dt. & Time', filterable: true },
             { field: 'StatusSort', title: 'Status', filterable: true },

             { title: 'Actions', filterable: true }]
}">
        </div>

        <script id="rowTmpl" type="text/html">

            <tr @*data-bind="style: { color: Status() == 'Completed' ? 'green' : 'black' }"*@>
@*<tr data-bind="loadingWhen: $root.viewModelHelper.isLoading">*@
                <td data-bind="style: { backgroundColor: (AnyDangerousGoods() == 'Yes' || AnyDangerousGoods() == 'A') ? '#FA370E' : '' }">
                    <div class="nowrap"><span data-bind="text: VCN" style="text-transform: capitalize;"></span></div>
                </td>
                <td>
                    <div class="nowrap"><span data-bind="text: VesselName" style="text-transform: capitalize;"></span></div>
                </td>
                <td>
                    <div class="nowrap"><span data-bind="text: ServiceTypeName"></span></div>
                </td>
                <td>
                    <div class="nowrap"><span data-bind="text: FromDate"></span></div>
                </td>
                <td>
                    <div class="nowrap"><span data-bind="text: WFStatusSupp"></span></div>
                </td>

                <td>
                    <div class="nowrap">
                        &nbsp; &nbsp;&nbsp; &nbsp;
                    @if (Model.HasViewPrivilege)
                    {
                        <a href="#" data-bind="click: $parent.ViewServiceRequest" title="View"><i class="fa fa-eye"></i></a> }
                        @if (Model.HasEditPrivilege)
                        {
                            <a href="#" data-bind="click: $parent.EditServiceRequest, visible: ((WFStatus() != 'Approve') && (WFStatus() != 'Reject') && (WFStatus() != 'Cancel') && (IsPrimaryAgent() == 'Y'))" title="Edit"><i class="fa fa-edit"></i></a>
                        }

                        <a href="#" data-target="#stack10" data-toggle="modal" data-bind="click: $parent.cancelReqst, visible: (IsPrimaryAgent() == 'Y' && (IsStartTime()) && WFStatus() == 'Approve') || (IsPrimaryAgent() == 'Y' && (WFStatus() == 'New Request' || WFStatus() == 'Update Request'))" title="Cancel"><i class="fa fa-ban"></i></a>

                        <a href="#" data-bind="click: $parent.viewWorkFlow, visible: (IsPrimaryAgent() == 'Y')" title="View Workflow"><i class="fa fa-stack-exchange"></i></a>

                    </div>
                </td>
            </tr>
        </script>
    </div>

    <div class="form-body" data-bind="with: supplementaryServiceRequestModel">
        <div class="form-group">
            <div id="stack1" class="modal fade" tabindex="-1" data-width="400">
                <div class="modal-dialog" style="width: 500px;">
                    <div class="modal-content">
                        <div class="modal-body">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                            <div class="scroller" style="height: 225px" data-always-visible="1" data-rail-visible1="1">
                                <fieldset>
                                    <legend id="spnTitile1">WorkFlow Remarks</legend>
                                    <div align="center">
                                        <textarea rows="3" cols="50" class="form-control" id="WorkFlowRemarks" style="width: 350px; height: 180px; text-align: center; margin: 0px 9.5px 0px 0px; resize: none;" data-bind="value: WorkFlowRemarks" disabled=""></textarea>
                                    </div>
                                </fieldset>
                            </div>
                        </div>
                        <br>
                        &nbsp;
                    </div>
                </div>
            </div>
        </div>
    </div>


</script>
<!-- Supplementary Service Request List ends here-->

