            </div>
            <div data-bind="kendoGrid: {
            data: FuelReceiptList,
            groupable: false,
            sortable: true,
            scrollable: true,
            filterable: { extra: false, operators: { string: { startswith: 'Starts with', eq: 'Is equal to', neq: 'Is not equal to' }, number: {  eq: 'Is equal to', neq: 'Is not equal to' } } },
            rowTemplate: 'rowTmpl',
            pageable: { pageSize: 5, pageSizes: 5 },
            useKOTemplates: true, columns: [{ field: 'ReceiptNoSort', title: 'Receipt No.', width: '120px', filterable: true }
                      , { field: 'RequisitionNoSort', title: 'Requisition No.', width: '150px', filterable: true }
                      , { field: 'CraftNameSort', title: 'Craft Name', width: '150px', filterable: true }                     
                      , { field: 'CraftTypeSort', title: 'Craft Type', width: '150px', filterable: true }
                      , { field: 'IMOSort', title: 'IMO', width: '80px', filterable: true, type:'number' }
                      , { field: 'OilTypeSort', title: 'Oil Type', width: '150px', filterable: true }
