@model IPMS.Domain.ValueObjects.PrivilegeVO

@{    
    ViewBag.Title = "FuelReceipt";
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";

 var fuelrequestionid = 0;
    var showViewOnLoad = false;
    if (!string.IsNullOrEmpty(ViewBag.ID))
    {
        fuelrequestionid = Convert.ToInt32(ViewBag.ID);
        showViewOnLoad = true;
    }
}

@section scripts
{
    <script src="~/Scripts/Application/Model/FuelReceiptModel.js"></script>
<script src="~/Scripts/Application/ViewModel/FuelReceiptViewModel.js"></script>
<script src="~/Scripts/Application/ViewModel/WorkflowTaskExecution.js"></script>
 <script src="~/Scripts/Lib/jspdf.min.js"></script>
}

@section ko_apply
{

var showOnLoad = false;
    if ("@showViewOnLoad" == "True")
    {
         showOnLoad = true;
    }
   var viewModel = new IPMSROOT.FuelReceiptViewModel("@fuelrequestionid", showOnLoad);
   
ko.applyBindingsWithValidation(viewModel, $("#FuelReceipt")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }});
}

<!-- BEGIN CONTENT -->
<div class="row">
    <div class="col-md-12">
        <h3 class="page-title"><span id="spnTitile"></span></h3>
        <ul class="page-breadcrumb breadcrumb"></ul>
    </div>
</div>
<div id="FuelReceipt">
    <div data-bind="visible: viewMode() == 'List', template: { name: 'List-template' }"></div>
    <div data-bind="visible: viewMode() == 'Form', template: { name: 'Form-template' }"></div>


</div>
<div class="clearfix"></div>
<div class="portlet-body">
