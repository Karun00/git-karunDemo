                                                    <div class="form-group">
                                                        <label class="control-label col-md-7">Total Time (mins):</label>
                                                        <div class="col-md-2">                                                         
                                                          <div class="col-md-5 control-label">
                                                                <label id="TotalTime" class="col-md-12 displaytxt" style="text-transform: capitalize;" data-bind="text: TotalTime"></label>
                                                            </div>                                                            
                                                        </div>
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                        <fieldset>
                                            <legend>Product Information</legend>
                                            <div class="form-group row">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-6">Density @@ 15 Degree Celsius:<span class="required">*</span></label>
                                                        <div class="col-md-5">                                                         
                                                            <input type="text" id="Densityat15DegCelsius" name="name" maxlength="15" autocomplete="off" data-required="1" class="form-control" onkeypress="return allowOnlyThreePositiveDigts(this,event);"   data-bind="value: Densityat15DegCelsius, enable: $parent.editableView, event: { keypress: $parent.validationHelper.ValidateNumeric }">
                                                            <span id="spandensity1" class="validationError" data-bind="validationMessage: Densityat15DegCelsius"></span>
                                                        </div>                                                       
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-6">Density @@ 20 Degree Celsius:<span class="required">*</span></label>
                                                        <div class="col-md-5">                                                         
                                                            <input type="text" id="Densityat20DegCelsius" name="name" maxlength="15" autocomplete="off" data-required="1" class="form-control" onkeypress="return allowOnlyThreePositiveDigts(this,event);" data-bind="value: Densityat20DegCelsius, enable: $parent.editableView, event: { keypress: $parent.validationHelper.ValidateNumeric }">
                                                            <span id="spandensity2" class="validationError" data-bind="validationMessage: Densityat20DegCelsius"></span>
                                                        </div> 
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-6">Flash Point:<span class="required">*</span></label>
                                                       <div class="col-md-5">                                                         
                                                            <input type="text" id="FlashPoint" name="name" maxlength="15" autocomplete="off" data-required="1" class="form-control" onkeypress="return allowOnlyThreePositiveDigts(this,event);"  data-bind="value: FlashPoint, enable: $parent.editableView, event: { keypress: $parent.validationHelper.ValidateNumeric }">
                                                            <span id="spanflashpoint" class="validationError" data-bind="validationMessage: FlashPoint"></span>
                                                        </div> 
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-6">Batch No.:<span class="required">*</span></label>
                                                        <div class="col-md-5">                                                         
                                                            <input type="text" id="BatchNo" name="name" maxlength="15" autocomplete="off" data-required="1" class="form-control" data-bind="value: BatchNo, enable: $parent.editableView">
                                                            <span id="BatchNo" class="validationError" data-bind="validationMessage: BatchNo"></span>
                                                        </div> 
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-6">Kinematic Visc. @@ 50 Deg Celsius:<span class="required">*</span></label>
                                                       <div class="col-md-5">                                                         
                                                            <input type="text" id="KinematicViscat50DegCelsius" name="name" maxlength="15" autocomplete="off" data-required="1" class="form-control" onkeypress="return allowOnlyThreePositiveDigts(this,event);"  data-bind="value: KinematicViscat50DegCelsius, enable: $parent.editableView, event: { keypress: $parent.validationHelper.ValidateNumeric }">
                                                            <span id="spankinematic" class="validationError" data-bind="validationMessage: KinematicViscat50DegCelsius"></span>
                                                        </div> 
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-6">Water Content (0-100%):<span class="required">*</span></label>
                                                       <div class="col-md-5">                                                         
                                                            <input type="text" id="WaterContent"  name="name" maxlength="7" autocomplete="off" intOnly="true" class="form-control" onkeypress="return allowOnlyThreePositiveDigts(this,event);" data-bind="value: WaterContent, enable: $parent.editableView, event: { keypress: $parent.validationHelper.ValidateNumeric }">
                                                            <span id="spanwater" class="validationError" data-bind="validationMessage: WaterContent"></span>
                                                        </div> 
                                                    </div>
                                                </div>
                                            </div>
                                            <div class="form-group row">
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-6">Sulphur Content:<span class="required">*</span></label>
                                                        <div class="col-md-5">                                                         
                                                            <input type="text" id="SulphurContent" name="name" maxlength="15" data-required="1" autocomplete="off" class="form-control" onkeypress="return allowOnlyThreePositiveDigts(this,event);"  data-bind="value: SulphurContent, enable: $parent.editableView, event: { keypress: $parent.validationHelper.ValidateNumeric }">
                                                            <span id="spansulpher" class="validationError" data-bind="validationMessage: SulphurContent"></span>
                                                        </div> 
                                                    </div>
                                                </div>
                                                <div class="col-md-6">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-6">Supplier:<span class="required">*</span></label>
                                                       <div class="col-md-5">                                                         
                                                            <input type="text" id="Supplier" name="name" maxlength="15" data-required="1" autocomplete="off" class="form-control" data-bind="value: Supplier, enable: $parent.editableView, event: { keypress: $parent.validationHelper.ValidateAlphabetsWithSpaces_keypressEvent }">
                                                            <span id="spansupplier" class="validationError" data-bind="validationMessage: Supplier"></span>
                                                        </div> 
                                                    </div>
                                                </div>
                                            </div>
                                        </fieldset>
                                        <div class="form-group row">
                                            <label class="control-label col-md-1">Remarks:</label>
                                            <div class="col-md-10">
                                               <textarea class="form-control" rows="2" id="Remarks" maxlength="500" data-bind="value: FuelRemarks, enable: $parent.editableView, event: { keypress: $parent.validationHelper.ValidateAlphaNumericWithSpaces} "></textarea>
                                             </div>
                                        </div>
                                </div>
                            </div>
                            <div class="btns-group form-actions fluid" data-bind="loadingWhen: $parent.viewModelHelper.isLoading">
                                <button type="submit" id="Save" class="btn green" value="POST VAL"
                                    data-bind="click: $parent.SaveFuelReceipt, visible: $parent.IsSave, disable: $parent.IsSaveUpdateDisabled">
                                    Save</button>
                                <button type="submit" class="btn green" data-bind="click: $parent.ModifyFuelReceipt, visible: $parent.IsUpdate, disable: $parent.IsSaveUpdateDisabled">Update</button>
                                <button type="submit" class="btn blue" data-bind="click: $parent.ResetFuelReceipt, visible: $parent.IsReset ">Reset</button>
                                <button type="button" onclick="" id="PrintOut" class="btn green" data-bind="click: $parent.PDFGeneration, visible: $parent.IsPrintEnable">Print Receipt</button>
                                <button type="submit" class="btn red" data-bind="click: $parent.CancelFuelReceipt">Cancel</button>
                            </div>

                             <div class="form-group">
                                <div class="form-actions fluid">
                                    @{Html.RenderPartial(Url.Content("~/Views/Shared/PendingTaskView.cshtml"));}
                                </div>
                            </div>

                        </div>
                    </div>
                </form>
            </div>
        </script>
    </div>

</div>
	