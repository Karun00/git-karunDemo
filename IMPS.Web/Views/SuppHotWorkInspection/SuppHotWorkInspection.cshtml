                    <label class="radio-inline">Yes</label>
                    <input name="HighProtection" class="hide-row" type="radio" value="N" data-bind="checked: HighProtection, enable: $root.editableView">
                    <label class="radio-inline hide-row">No</label>
                    <span id="spanHighProtection" class="validationError"></span>
