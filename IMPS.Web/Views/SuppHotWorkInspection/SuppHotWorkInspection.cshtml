            <div class="col-md-2">

                <div class="radio-list">
                    <input type="radio" class="hide-row" name="EquipmentStandby" value="Y" checked="checked" data-bind="checked: EquipmentStandby, enable: $root.editableView">
                    <label class="radio-inline">Yes</label>
                    <input name="EquipmentStandby" class="hide-row" type="radio" value="N" data-bind="checked: EquipmentStandby, enable: $root.editableView">
                    <label class="radio-inline hide-row">No</label>
                    <span id="spanEquipmentStandby" class="validationError"></span>
                </div>
