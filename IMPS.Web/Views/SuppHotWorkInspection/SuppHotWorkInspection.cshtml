</div>
<div class="clearfix"></div>

<!-- Grid / List Starts here -->
<script type="text/html" id="SuppHotWorkInspectionList-template">
    <div class="portlet-body" data-bind="loadingWhen: $root.viewModelHelper.isLoading">
        <div class="table-toolbar">
            <div id="tablewrapper15">
                @*<div id="tableheader">
                    <a class="button gbtngreen" data-bind="click: $root.addSuppHotWorkInspection">Add New <i class="fa fa-plus"></i></a>
                </div>*@
                <div data-bind="kendoGrid: {
    data: SuppHotWorkInspectionList,
    groupable: false,
    sortable: true,
    scrollable: true,
    filterable: { extra: false, operators: { string: { startswith: 'Starts with', eq: 'Is equal to', neq: 'Is not equal to', contains: 'Contains', doesnotcontain: 'Does not Contain' } } },
    rowTemplate: 'rowTmpl',
    pageable: { pageSize: 5, pageSizes: 5 },
    useKOTemplates: true,
    columns: [{ field: 'VCNSort', title: 'VCN', filterable: true, width: '120px' },
              { field: 'ServiceTypeSort', title: 'Service Type', filterable: true, width: '200px' },
              { field: 'VesselNameSort', title: 'Name of Vessel', filterable: true, width: '150px' },
              { field: 'RequestDtAndTimeSort', title: 'Request Date', filterable: true, width: '160px' },
              { field: 'RequestDtAndTimeSort', title: 'Request From', filterable: true, width: '160px' },
              { field: 'ToDate', title: 'Request To', filterable: true, width: '160px' },
              { field: 'LocationName', title: 'Location', filterable: true, width: '150px' },
              { title: 'DG Status', width: '100px' },
              { field: 'permitnoSort', title: 'Permit No.', filterable: true, width: '150px' },
              { title: 'PermitStatusSort', title: 'Permit Status', filterable: false, width: '100px' },
              { title: 'Actions', filterable: false, width: '150px' }]
}">
                </div>

                <script id="rowTmpl" type="text/html">
                    <tr>
                        <td data-bind="style: { backgroundColor: (AnyDangerousGoodsonBoard() == 'A') ? '#FA370E' : '' }">
                            <div class="nowrap">
                                <span data-bind="text: VCN"></span>
                            </div>
                        </td>
                        <td>
                            <div class="nowrap">
                                <span data-bind="text: ServiceTypeName "></span>
                            </div>
                        </td>
                        <td>
                            <div class="nowrap">
                                <span data-bind="text: VesselName"></span>
                            </div>
                        </td>
                        <td>
                            <div class="nowrap">
                                <span data-bind="text: moment(FromDate()).format('YYYY-MM-DD HH:mm')"></span>
                            </div>
                        </td>
                        <td>
                            <div class="nowrap">
                                <span data-bind="text: moment(FromDate()).format('YYYY-MM-DD HH:mm') "></span>
                            </div>
                        </td>
                        <td>
                            <div class="nowrap">
                                <span data-bind="text: moment(ToDate()).format('YYYY-MM-DD HH:mm')"></span>
                            </div>
                        </td>
                        <td>
                            <div class="nowrap">
                                <span data-bind="text: LocationName"></span>
                            </div>
                        </td>
                        <td>
                            <div class="nowrap">
                                <span data-bind="text: AnyDangerousGoodsonBoardt"></span>
                            </div>
                        </td>
                        <td>
                            <div class="nowrap">
                               @* <span data-bind="text: HWPNText"></span>*@
                               <span data-bind="text: HWPN"></span>
                            </div>
                        </td>
                        <td>
                            <div class="nowrap">
                                <span data-bind="text: PermitStatust"></span>
                            </div>
                        </td>
                         
                        <td>
                            <div class="nowrap">
                                @if (Model.HasEditPrivilege)
                                {
                                    <a href="#" data-bind="click: $parent.editSuppHotWorkInspection, visible:((PermitStatust() != 'Verified')||(value() !='Y')) ? true : false" title="Edit"><i class="fa fa-edit"></i></a>
                                }
                                @if (Model.HasViewPrivilege)
                                {
                                    <a href="#" data-bind="click: $parent.viewSuppHotWorkInspection" title="View"><i class="fa fa-eye"></i></a>}
      
                        @if (Model.HasEditPrivilege)
                        {
                            <a href="#" data-bind="click: $parent.verifyEditSuppHotWorkInspection, visible: ((PermitStatust() !='Verified')||(value() !='Y'))?true:false" title="Verify"><i class="fa fa-WFVE"></i></a>
                        }     
                   
                            </div>
                        </td>
                    </tr>
                </script>
            </div>
        </div>
    </div>
</script>
<!-- Grid / List Endss here -->

