        <div class="form-group">
            <label class=" col-md-10">
                Barricades Required:
            </label>
            <div class="col-md-2">

                <div class="radio-list">
                    <input type="radio" class="hide-row" name="BarricadesRequired" value="Y" checked="checked" data-bind="checked: BarricadesRequired, enable: $root.editableView">
                    <label class="radio-inline">Yes</label>
                    <input class="hide-row" name="BarricadesRequired" type="radio" value="N" data-bind="checked: BarricadesRequired, enable: $root.editableView">
                    <label class="radio-inline hide-row">No</label>
                    <span id="spanBarricadesRequired" class="validationError"></span>
                </div>
            </div>
