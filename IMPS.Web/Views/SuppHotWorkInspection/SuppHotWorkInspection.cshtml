            <div class="col-md-2">
                <div class="radio-list">
                    <input type="radio" class="hide-row" name="FireDetectors" value="Y" checked="checked" data-bind="checked: FireDetectors, enable: $root.editableView">
                    <label class="radio-inline">Yes</label>
                    <input name="FireDetectors" class="hide-row" type="radio" value="N" data-bind="checked: FireDetectors, enable: $root.editableView">
                    <label class="radio-inline hide-row">No</label>
                    <span id="spanFireDetectors" class="validationError"></span>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="form-group">
