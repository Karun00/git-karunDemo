            <div class="col-md-2">

                <div class="radio-list">
                    <input type="radio" class="hide-row" name="ConductiveMetals" value="Y" checked="checked" data-bind="checked: ConductiveMetals, enable: $root.editableView">
                    <label class="radio-inline">Yes</label>
                    <input name="ConductiveMetals" class="hide-row" type="radio" value="N" data-bind="checked: ConductiveMetals, enable: $root.editableView">
                    <label class="radio-inline hide-row">No</label>
                    <span id="spanConductiveMetals" class="validationError"></span>
                </div>
            </div>
        </div>
