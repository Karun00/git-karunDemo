@model IPMS.Domain.ValueObjects.PrivilegeVO
@{   
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";
}

@section scripts
{
    <script src="~/Scripts/Application/Model/SuppHotWorkInspectionModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/SuppHotWorkInspectionViewModel.js"></script>
    <script src="~/Scripts/Lib/jspdf.min.js"></script>
}
@section ko_apply
{
var viewModel = new IPMSROOT.SuppHotWorkInspectionViewModel();  
ko.applyBindingsWithValidation(viewModel, $("#SuppHotWorkInspection")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }});
}

<style>
