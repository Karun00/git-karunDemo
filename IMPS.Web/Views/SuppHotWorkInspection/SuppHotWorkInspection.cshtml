<script id="customMessageTemplate" type="text/html">
    <span class="validationError" data-bind='validationMessage: field'></span>
</script>

<script type="text/html" id="SuppHotWorkInspectionForm-template">

<div class="portlet portlet-body form" data-bind="loadingWhen: $root.viewModelHelper.isLoading">
<form action="#" class="form-horizontal" id="submit_form">
<div class="form-wizard" data-bind='validationOptions: { messageTemplate: "customMessageTemplate" }'>
<div class="form-body" data-bind="with: SuppHotWorkInspectionModel">
<div class="form-group" id="divReqdtls">
<div data-bind="with: SuppHotWorkInspectionVO">
    <h6>
        <label>Permit No:</label>
        <label id="idHWPN" data-bind="text: HWPN"></label>
    </h6>
</div>
<fieldset>
<fieldset>
    <legend>VCN Details</legend>
    <div class="row" data-bind="with: VesselData">
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">VCN:</label>
                <div>
                    <label id="PrintVCN" class="control-label displaytxt" data-bind="text: VCN"></label>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Vessel Name:</label>
                <div>
                    <label id="PrintVesselName" class="control-label displaytxt" data-bind="text: VesselName "></label>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Current Berth:</label>
                <div>
                    @* data-bind="with: VesselData" <label class="control-label displaytxt"  data-bind="text: BerthName "></label>*@
                </div>
            </div>
        </div>
    </div>

    <div class="row">
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Service Request:</label>

                <label id="PrintServiceRequest" class="control-label displaytxt" data-bind="text: ServiceTypeName "></label>

            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Request Date:</label>
                <div>
                    <label id="PrintRequestDate" class="control-label displaytxt" data-bind="text: moment(FromDate()).format('YYYY-MM-DD HH:mm')"></label>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Service Requested at Berth:</label>
                <div>
                    <label id="PrintBerthName" class="control-label displaytxt" data-bind="text: BerthName "></label>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Location:</label>
                <div data-bind="with: SuppHotColdWorkPermitsVO">
                    <label id="PrintLocationName" class="control-label displaytxt" data-bind="text: LocationName "></label>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Request From:</label>

                <label id="PrintFromDate" class="control-label displaytxt" data-bind="text: moment(FromDate()).format('YYYY-MM-DD HH:mm')"></label>

            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Request To:</label>

                <label id="PrintToDate" class="control-label displaytxt" data-bind="text: moment(ToDate()).format('YYYY-MM-DD HH:mm')"></label>

            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Gas Free Certificate Issuing Authority :</label>
                <div data-bind="with: SuppHotColdWorkPermitsVO">
                    <label id="PrintGassFreeIssuingAuthority" class="control-label displaytxt" data-bind="text: GassFreeIssuingAuthority "></label>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Gas Free Certificate Available:</label>
                <div data-bind="with: SuppHotColdWorkPermitsVO">
                    <label id="PrintGassCertificateAvailable" class="control-label displaytxt" data-bind="text: GassCertificateAvailable "></label>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Gas Free Certificate Validity:</label>
                <div data-bind="with: SuppHotColdWorkPermitsVO">
                    <label id="PrintGassFreeCertificateValidity" class="control-label displaytxt" data-bind="text: GassFreeCertificateValidity"></label>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Dangerous Goods Status:</label>
                <div class="col-md-6 control-label">
                    <label id="PrintAnyDangerousGoodsonBoardt" class="col-md-6 displaytxt" data-bind="text: AnyDangerousGoodsonBoardt "></label>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">Class:</label>
                <div class="col-md-6 control-label">
                    <label id="PrintDangerousGoodsClass" class="col-md-6 displaytxt" data-bind="text: DangerousGoodsClass "></label>
                </div>
            </div>
        </div>
        <div class="col-md-4">
            <div class="form-group">
                <label class="control-label col-md-6">UN No.:</label>
                <div class="col-md-6 control-label">
                    <label id="PrintUNNo" class="col-md-6 displaytxt" data-bind="text: UNNo "></label>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
        <p>&nbsp;</p>
    </div>
</fieldset>
<fieldset data-bind="with: SuppHotColdWorkPermitsVO">
    <legend>Uploaded Documents</legend>
    <div class="row">
        <div class="col-md-12">
            <table class="table">
                <tbody data-bind="foreach: SuppHotColdWorkPermitDocumentsVO">
                <tr>
                    <td>
                        <a data-bind="  text: FileName, attr: { href: '/FileDownload/Download/' + DocumentID() }, enable: $parent.IsEditable"></a>
                    </td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>

</fieldset>

<fieldset data-bind="with: SuppHotWorkInspectionVO">
<legend>Inspection Details</legend>
<div class="row">
    <div class="col-md-12">
        <div class="form-group">
            <label class="col-md-10">
                The Person Conducting the hot work is familiar with the emergency evacuation procedures:
            </label>
            <div class="col-md-2">
                <div class="radio-list">
                    <input type="radio" class="hide-row" name="EmergencyProcedure" value="Y" checked="checked" data-bind="checked: EmergencyProcedure, enable: $root.editableView">
                    <label class="radio-inline">Yes</label>
                    <input name="EmergencyProcedure" class="hide-row" type="radio" value="N" data-bind="checked: EmergencyProcedure, enable: $root.editableView">
                    <label class="radio-inline hide-row">No</label>
                    <span id="spanEmergencyProcedure" class="validationError"></span>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="form-group">
            <label class="col-md-10">
                Is the hot work area and surrounding area free of any combustibles - 10 meters minimum (Fire Risk Assessment):
            </label>
            <div class="col-md-2">
                <div class="radio-list">
                    <input type="radio" class="hide-row" name="FireRiskAssessment" value="Y" checked="checked" data-bind="checked: FireRiskAssessment, enable: $root.editableView">
                    <label class="radio-inline">Yes</label>
                    <input name="FireRiskAssessment" class="hide-row" type="radio" value="N" data-bind="checked: FireRiskAssessment, enable: $root.editableView">
                    <label class="radio-inline hide-row">No</label>
                    <span id="spanFireRiskAssessment" class="validationError"></span>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="form-group">
            <label class="col-md-10">
                Has the hot work area been tested for flammable gases:
            </label>
            <div class="col-md-2">
                <div class="radio-list">
                    <input type="radio" class="hide-row" name="FlammableGases" value="Y" checked="checked" data-bind="checked: FlammableGases, enable: $root.editableView">
                    <label class="radio-inline">Yes</label>
                    <input class="hide-row" name="FlammableGases" type="radio" value="N" data-bind="checked: FlammableGases, enable: $root.editableView">
                    <label class="radio-inline hide-row">No</label>
                    <span id="spanFlammableGases" class="validationError"></span>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="form-group">
            <label class="col-md-10">
                Is continuous gas monitoring required and agree upon:
            </label>
            <div class="col-md-2">
                <div class="radio-list">
                    <input class="hide-row" type="radio" name="GasMonitoring" value="Y" checked="checked" data-bind="checked: GasMonitoring, enable: $root.editableView">
                    <label class="radio-inline">Yes</label>
                    <input class="hide-row" name="GasMonitoring" type="radio" value="N" data-bind="checked: GasMonitoring, enable: $root.editableView">
                    <label class="radio-inline hide-row">No</label>
                    <span id="spanGasMonitoring" class="validationError"></span>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-md-12">
        <div class="form-group">
            <label class=" col-md-10">
