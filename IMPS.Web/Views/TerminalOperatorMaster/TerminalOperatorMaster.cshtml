@model IPMS.Domain.ValueObjects.PrivilegeVO

@{
    ViewBag.Title = "Terminal Operator Master";
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";
}
@section scripts
{
    <script src="~/Scripts/Application/Model/TerminalOperatorModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/TerminalOperatorViewModel.js"></script>
}

@section ko_apply
{
var viewModel = new IPMSROOT.TerminalOperatorViewModel();
ko.applyBindingsWithValidation(viewModel, $("#TerminalOperatorMaster")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }});
}

<!-- BEGIN CONTENT -->
<div class="row">
    <div class="col-md-12">
        <h3 class="page-title"><span id="spnTitle"></span></h3>
        <ul class="page-breadcrumb breadcrumb"></ul>
    </div>
</div>

<div id="TerminalOperatorMaster">
    <div data-bind="visible: viewMode() == 'List', template: { name: 'List-template' }"></div>
    <div data-bind="visible: viewMode() == 'Form', template: { name: 'Form-template' }"></div>
</div>

<!-- terminaloperatorList  and Serach content starts here-->
<div class="clearfix"></div>

<div class="portlet-body">
    <div class="table-toolbar">
        <script type="text/html" id="List-template">
            <div data-bind="loadingWhen: $root.viewModelHelper.isLoading">
                <div id="tableheader">
                    @if (Model.HasAddPrivilege)
                    {
                        <a class="button gbtngreen" data-bind="click: $root.addTerminalOperator">Add New <i class="fa fa-plus"></i></a>
                    }
                </div>
                <div data-bind="kendoGrid: {
    data: terminaloperatorList,
    groupable: false,
    sortable: true,
    scrollable: true,
    filterable: {
        extra: false,
        operators: {
            string: {
                startswith: 'Starts with',
                eq: 'Is equal to',
                neq: 'Is not equal to'
            }
        }
    },
    rowTemplate: 'rowTmpl',
    pageable: { pageSize: 5, pageSizes: 5 },
    useKOTemplates: true,
    columns: [{ field: 'TerminalOperatorIDSort', title: 'TO Code', width: 58, filterable: true },
             { field: 'RegisteredNameSort', title: 'TO Rep. Name', width: 80, filterable: true },
             { field: 'TradingNameSort', title: 'TO Trading Name', width: 90, filterable: true },
             { field: 'RegistrationNumberSort', title: 'Registration No.', width: 80, filterable: true },
             { field: 'RegistrationDateSort', title: 'Registration Date', width: 120, filterable: true },
             { field: 'ValidityDateSort', title: 'Validity Date', filterable: true, width: 120 },
             { field: 'LicenLicensedForStringSort', title: 'Lisenced For', filterable: true, width: 120 },
             { title: 'Status', filterable: false, width: 50 },
             { title: 'Actions', filterable: false, width: 45 }]
}">
                </div>

                <script id="rowTmpl" type="text/html">
                    <tr>
                        <td data-bind="text: TerminalOperatorID"></td>
                        <td>
                            <div class="nowrap" data-bind="text: RegisteredName"></div>
                        </td>
                        <td data-bind="text: TradingName"></td>
                        <td data-bind="text: RegistrationNumber"></td>
                        <td>
                            <div class="nowrap" data-bind="text: moment(RegistrationDate()).format('YYYY-MM-DD')"></div>
                        </td>
                        <td data-bind="text: moment(ValidityDate()).format('YYYY-MM-DD')"></td>
                        <td data-bind="text: LicenLicensedForString"></td>
                        <td data-bind="text: Statust"></td>
                        <td>
                            <div class="nowrap">
                                @if (Model.HasViewPrivilege)
                                {
                                    <a href="javascript:;" title="View" data-bind="click: $parent.viewTerminalOperator"><i class="fa fa-eye"></i></a>
                                }&nbsp; &nbsp;
                                @if (Model.HasEditPrivilege)
                                {
                                    <a href="javascript:;" title="Edit" data-bind="click: $parent.EditTerminalOperator"><i class="fa fa-edit"></i></a>
                                }&nbsp; &nbsp;
                            </div>
                        </td>
                    </tr>
                </script>
            </div>
        </script>
    </div>
</div>

<script id="customMessageTemplate" type="text/html">
    <span class="validationError" data-bind='validationMessage: field'></span>
</script>

<!--Terminaloperator form for AddForm / EditForm and ViewForm starts here-->
<script type="text/html" id="Form-template">
    <div class="portlet portlet-body form">
        <form action="#" id="form_sample_1" class="form-horizontal">
