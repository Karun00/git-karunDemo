                                                    <div class="col-md-11">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-4">
                                                                Postal Code:<span class="required">*</span>
                                                            </label>
                                                            <div class="col-md-5">
                                                                <input id="PostalCode" type="text" onblur="PostalCodeChange(this);" onchange="PostalCodeChange(this);" class="form-control" data-bind=" value: PostalCode, event: { keypress: Validatenumeric, keyup: $root.AddessKeyupEvents }, enable: $root.IsCodeEnable" name="PostalCode" maxlength="15" />
                                                                <span id="errPostalCodeMsg" class="validationError" data-bind="visible: $root.isPostalCodeMsg"></span>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-md-11">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-4">
                                                            Telephone No.:<span class="required">*</span>
                                                        </label>
                                                        <div class="col-md-3">
                                                            <input id="telephoneNo" type="text" class="form-control" data-bind="value: TelephoneNo1, event: { keypress: Validatenumeric }, enable: $root.IsCodeEnable" name="TelephoneNo1" maxlength="10" />
                                                        </div>
                                                        <div class="col-md-5">
                                                        </div>
                                                    </div>
                                                </div>

                                                <div class="col-md-11">
                                                    <div class="form-group">
                                                        <label class="control-label col-md-4">
                                                            Fax No.:<span class="required">*</span>
                                                        </label>
                                                        <div class="col-md-5">
