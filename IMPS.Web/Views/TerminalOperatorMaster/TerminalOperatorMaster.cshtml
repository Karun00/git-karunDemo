                                                                                    <div class="col-md-6">
                                                                                        <input type="checkbox" name="CargType" data-bind="checkedValue: $data.SubCatCode, checked: $root.terminaloperatorModel().operationalCargotypes, enable: $root.IsCodeEnable">
                                                                                        <label data-bind="text: SubCatName"></label>
                                                                                    </div>
                                                                                </div>

                                                                            </td>
                                                                        </tr>
                                                                    </tbody>

                                                                </table>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>

                                            </fieldset>
                                        </div>
                                    </div>
                                </div>

                                <div class="clearfix">
                                    <br />
                                    <br />
                                </div>
                            </fieldset>

                        </div>
                        <div class="btns-group form-actions fluid" data-bind="loadingWhen: $root.viewModelHelper.isLoading">
                            <button type="submit" class="btn green" title="Save" data-bind=" click: $parent.SaveTerminalOperator, visible: $parent.IsSave">Save</button>
                            <button type="submit" class="btn green" title="Update" data-bind="click: $parent.UpdateTerminalOperator, visible: $parent.IsUpdate">Update</button>
                            <button type="submit" class="btn blue" title="Reset" data-bind="click: $parent.ResetTerminalOperators, visible: $parent.IsReset">Reset</button>
                            <button type="submit" class="btn red button-submit" title="Cancel" data-bind="click: $parent.CancelTerminalOperator">Cancel</button>
                        </div>
                    </div>
                </div>
            </div>
        </form>
    </div>
</script>
