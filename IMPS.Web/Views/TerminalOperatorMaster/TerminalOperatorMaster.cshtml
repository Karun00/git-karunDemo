                                            <label class="control-label col-md-5">Trading Name:<span class="required">*</span></label>
                                            <div class="col-md-6">
                                                <input id="TradingName" type="text" class="form-control" data-bind="value: TradingName, event: { keypress: $root.ValidateAlphabetsWithSpaces }, enable: $parent.IsCodeEnable " name="TradingName" maxlength="100" />
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="row">
                                    <div class="col-md-6">
                                        <div class="form-group">
                                            <label class="control-label col-md-5">Registration Number:<span class="required">*</span></label>
                                            <div class="col-md-6">
