@model IPMS.Domain.ValueObjects.PrivilegeVO
@{
    ViewBag.Title = "Vessel Registration";
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";

    var vcn = "";
    var showViewOnLoad = false;
    if (!string.IsNullOrEmpty(ViewBag.IMONo))
    {
        vcn = ViewBag.IMONo;
        showViewOnLoad = true;
    }
}
<script id="customMessageTemplate" type="text/html">
    <span class="validationError" data-bind='validationMessage: field'></span>
</script>

@section scripts
{
    <script src="~/Scripts/Application/Model/VesselRegistrationModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/VesselRegistrationViewModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/WorkflowTaskExecution.js"></script>
}

@section ko_apply
{
    var showOnLoad = false;
    if ("@showViewOnLoad" == "True")
    {
         showOnLoad = true;
    }   
    var viewModel = new IPMSROOT.VesselRegistrationViewModel("@vcn", showOnLoad);
	ko.applyBindingsWithValidation(viewModel, $("#divVesselReg")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }}   );
 }

<style>
    select {
        text-transform: none;
        width: 100%;
        border-radius: 2px;
    }
</style>
<div class="row">
    <div class="col-md-12">
        <!-- BEGIN PAGE TITLE & BREADCRUMB-->
        <h3 class="page-title" id="spnTitile">Vessel Registration
        </h3>
        <ul class="page-breadcrumb breadcrumb"></ul>
    </div>
</div>

<div id="divVesselReg">
    <div data-bind="visible: viewMode() == 'List', template: { name: 'VesselRegistrationList-template' }"></div>
    <div data-bind="visible: viewMode() == 'Form', template: { name: 'VesselRegistrationForm-template' }"></div>
</div>

<script type="text/html" id="VesselRegistrationList-template">

    <div class="portlet-body">

        <div class="table-toolbar">

            <div>
                <div class="portlet box advbg">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="fa fa-reorder"></i>Advance Search
                        </div>
                        <div class="tools">
                            <a href="javascript:;" class="collapse"></a>
                        </div>
                    </div>
                    <div class="portlet-body" data-bind="with: VesselSearchDetails">
                        <div class="form-wizard form-horizontal">
                            <div class="form-body">
                                <div class="form-group">
                                    <div class="row">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label class="control-label col-md-6">IMO No.:</label>
                                                <div class="col-md-6">
                                                    <input type="text" id="SerchIMONo" class="form-control" data-bind="value: IMONo, event: { keypress: $root.ValidateNumeric }" maxlength="7">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-5">
                                            <div class="form-group">
                                                <label class="control-label col-md-5">Vessel Nationality/Flag:</label>
                                                <div class="col-md-7">
                                                    <select name="documents" id="SerchNationality" class="form-control" data-bind="options: $root.VesselReferenceDtl().ddlVesselNationality, optionsText: 'VNName', optionsValue: 'VNCode', value: VesselNationality, optionsCaption: 'Select...'">
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group1">
                                                <label class="control-label col-md-3">Vessel Type:</label>
                                                <div class="col-md-9">
                                                    <select class="form-control" id="SerchVesselType" data-bind="options: $root.VesselReferenceDtl().ddlVesselTypes, optionsText: 'vesselTypeName', optionsValue: 'vesselTypeCode', value: VesselType, optionsCaption: 'Select...'">
                                                    </select>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label class="control-label col-md-6">Port of Registry:</label>
                                                <div class="col-md-6">
                                                    <input id="SearchPOR" data-bind="kendoAutoComplete: { data: $root.VesselReferenceDtl().ddlPOR, dataTextField: 'PortName', dataValueField: 'PortCode', value: PortCode, enable: $root.isVesselNationalityEnable, select: $root.VesselSelect }" />
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-5">
                                            <div class="form-group1">
                                                <label class="control-label col-md-5">Vessel Name:</label>
                                                <div class="col-md-7">
                                                    <input type="text" id="SearchVesselName" class="form-control" data-bind="value: VesselName" maxlength="30">
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group1">
                                                <label class="control-label col-md-3">Call Sign:</label>
                                                <div class="col-md-9">
                                                    <input type="text" id="SearchCallSign" class="form-control" data-bind="value: CallSign" maxlength="30">
                                                </div>
                                            </div>
                                        </div>

                                    </div>
                                    <div class="row">
                                        <div class="col-md-12">
                                            <div class="col-md-6"></div>
                                            <div class="col-md-6 fluid">
                                                <button type="submit" class="btn btn-xs green" data-bind="click: $root.GetVesselData">Get Data</button>
                                                <button type="submit" class="btn btn-xs blue" data-bind="click: $root.ResetSearchDtl">Reset</button>
                                            </div>
                                        </div>
                                    </div>

                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>

            <div>
                @if (Model.HasAddPrivilege)
                {
                    <a class="button gbtngreen" data-bind="click: $root.addvesselreg">Add New <i class="fa fa-plus"></i></a>
                }
                <div class="search" style="border: none; padding: 0; margin-top: 5px;">
                    <select id="columns" onchange="sorter.search('query')" style="display: none;"></select>
                </div>
            </div>

            <div id="vesselList" data-bind="kendoGrid: {
    data: VesselRegistrationList,
    sortable: true,
    scrollable: true,
    filterable: {
        extra: false, operators: {
            string: { startswith: 'Starts with', eq: 'Is equal to', neq: 'Is not equal to' },
            date: {
                gte: 'Is after or equal to',
                gt: 'Is after',
                lte: 'Is before or equal to',
                lt: 'Is before'
            }
        }
    },
    rowTemplate: 'rowTmpl',
    pageable: { pageSize: 20, pageSizes: 5 },
    useKOTemplates: true,
    columns: [{ field: 'VesselNameSort', title: 'Vessel Name', width: 65, filterable: true },
        { field: 'IMONoSort', title: 'IMO No.', width: 80, filterable: true },
        { field: 'VesseltypeSort', title: 'Vessel Type', width: 60, filterable: true },
        { field: 'CallSignSort', title: 'Call Sign', width: 60, filterable: true },
        { field: 'SubmissionDateSort', title: 'Submission Date', width: 80, filterable: true, type: 'date' },
        { title: 'Status', filterable: true, width: 50 },
        { title: 'Actions', width: 35, filterable: false }]
}">
            </div>

            <script id="rowTmpl" type="text/html">
                <tr data-bind="loadingWhen: $root.viewModelHelper.isLoading">
                    <td>
                        <span data-bind="text: VesselName"></span>
                    </td>
                    <td>
                        <div class="nowrap"><span data-bind="text: IMONo"></span></div>
                    </td>
                    <td>
                        <div class="nowrap"><span data-bind="text: VesselTypeName"></span></div>
                    </td>
                    <td>
                        <div class="nowrap"><span data-bind="text: CallSign"></span></div>
                    </td>
                    <td>
                        <div class="nowrap"><span data-bind="text: moment(CreatedDate()).format('YYYY-MM-DD HH:mm')"></span></div>
                    </td>
                    <td>
                        <div class="nowrap"><span data-bind="text: WFStatus"></span></div>
                    </td>
                    <td>
                        <div class="nowrap">
                            @if (Model.HasViewPrivilege)
                            {
                                <a href="#" data-bind="click: $parent.viewData" title="View">
                                    <i class="fa fa-eye"></i></a>
                            }
                            @if (Model.HasEditPrivilege)
                            {
                                <a href="#" data-bind="click: $parent.editData, visible: IsVisible() == 'F' ? false : (RecordStatus() == 'I' ? false : true)" title="Edit"><i class="fa fa-edit"></i></a>
                            }
                            <a href="#" data-bind="click: $parent.viewWorkFlow" title="View Workflow"><i class="fa fa-stack-exchange"></i></a>
                        </div>
                    </td>
                </tr>
                <div data-bind="visible: VesselRegistrationList().length === 0,">
                    No Records Found...
                </div>

            </script>

            <div style="height: 10px;">
            </div>

            <div class="row" id="msgNote">
                <div style="color: #f00; font-size: 11px;">Note: First 200 vessels are displayed sorted by name, please use advanced search feature to refine your search</div>
            </div>
        </div>
    </div>

    <div class="form-body" data-bind="with: VesselRegisrationDetModel">
        <div class="form-group">
            <div id="stack1" class="modal fade" tabindex="-1" data-width="400">
                <div class="modal-dialog" style="width: 500px;">
                    <div class="modal-content">
                        <div class="modal-body">
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                            <div class="scroller" style="height: 225px" data-always-visible="1" data-rail-visible1="1">
                                <fieldset>
                                    <legend id="spnTitile1">WorkFlow Remarks</legend>
                                    <div align="center">
                                        <textarea rows="3" cols="50" class="form-control" id="WorkFlowRemarks" style="width: 350px; height: 180px; text-align: center; margin: 0px 9.5px 0px 0px; resize: none;" data-bind="value: WorkFlowRemarks" disabled=""></textarea>
                                    </div>
                                </fieldset>
                            </div>
                        </div>
                        <br>
                        &nbsp;
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="row">
    </div>
</script>
<div class="clearfix"></div>

<script type="text/html" id="VesselRegistrationForm-template">
    <div class="portlet-body">
        <div class="row">
            <div class="col-md-12" data-bind="with: VesselRegisrationDetModel">
                <div class="tabbable tabbable-custom" data-bind='validationOptions: { messageTemplate: "customMessageTemplate" }'>
                    <ul class="nav nav-tabs" id="mainmenu">

                        <li class="active" id="li_tab0" data-bind="click: $root.vesseldettab">
                            <a href="#tab_0" data-toggle="tab">Vessel Details</a>
                        </li>

                        <li id="li_tab1" data-bind="click: $root.vesselparamtab">
                            <a href="#tab_1">Vessel Parameters</a>
                        </li>

                        <li id="li_tab2" data-bind="click: $root.vesselcerttab">
                            <a href="#tab_2">Statutory Certificate Details</a>
                        </li>

                        <li id="li_tab3" data-bind="click: $root.vesselothertab">
                            <a href="#tab_3">Other Details</a>
                        </li>

                    </ul>
                    <div class="tab-content">
                        <div class="tab-pane active" id="tab_0">
                            <form action="#" class="form-horizontal" id="submit_form">
                                <div class="form-wizard">
                                    <div class="form-body">

                                        <div class="form-group">
                                            <fieldset>
                                                <div class="clearfix">
                                                    <br>
                                                </div>
                                                <div class="row col-md-12">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">IMO No.:<span class="required">*</span></label>
                                                            <div class="col-md-6">
                                                                <div class="col-md-4">
                                                                    <input type="text" class="form-control" id="IMONo" data-bind="value: IMONo, event: { keypress: $root.ValidateNumeric, change: $parent.ValidateIMONO, keyup: HandleKeyUp }, enable: $root.isIMONoEnable" maxlength="7" autofocus>
                                                                </div>
                                                               @* <div class="col-md-5" style="margin-left: 2px;">
                                                                    <input type="button" id="Go" class="btn btn-xs blue" value="Go" data-bind="click: $root.GetIMOFromService, visible: $root.isIMONoEnable">
                                                                </div>*@
                                                                <span id="spanimono" class="validationError" style="display: none"></span>
                                                            </div>
                                                            @*  change: $parent.ValidateChangevalueLOA 
                                                            <div class="col-md-3">
                                                                <button class="btn1 blue" type="button">Get Details</button>
                                                            </div>
                                                            *@
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">Call Sign:<span class="required">*</span></label>
                                                            <div class="col-md-6">
                                                                <input type="text" id="CallSign" class="form-control" data-bind="value: CallSign, enable: $root.isCallSignEnable" maxlength="30">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">Ex-Call Sign:</label>
                                                            <div class="col-md-6">
                                                                <input type="text" id="ExCallSign" class="form-control" data-bind="value: ExCallSign, enable: $root.isExCallSignEnable" maxlength="30">
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">Vessel Name:<span class="required">*</span></label>
                                                            <div class="col-md-6">
                                                                <input type="text" id="VesselName" class="form-control" data-bind="value: VesselName, enable: $root.isVesselNameEnable" maxlength="30">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">Classification Society:</label>
                                                            <div class="col-md-6">
                                                                <div class="form-control">
                                                                    <select name="documents" id="ClassificationSociety" class="form-control" data-bind="options: $root.VesselReferenceDtl().ddlClassificationSocities, optionsText: 'ClsSocityName', optionsValue: 'ClsSocityCode', enable: $root.isClassificationSocitiesEnable, value: ClassificationSociety, optionsCaption: 'Select...'">
                                                                    </select>
                                                                    <span class="validationError" data-bind='validationMessage: ClassificationSociety, enable: $root.isClassificationSocitiesEnable'></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">Vessel Type:<span class="required">*</span></label>
                                                            <div class="col-md-6">
                                                                <div class="form-control">
                                                                    <div class="form-control">
                                                                        <select id="VesselType" class="form-control" data-bind="options: $root.VesselReferenceDtl().ddlVesselTypes, optionsText: 'vesselTypeName', optionsValue: 'vesselTypeCode', enable: $root.isVesselTypeEnable, value: VesselType, optionsCaption: 'Select...', event: { change: ChangeVesselType }">
                                                                        </select>
                                                                        <span id="spanmaintenance" class="validationError"></span>

                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <label class="control-label col-md-6">TEU Capacity:<span id="reqTEUCapacity" class="required">*</span></label>
                                                        <div class="col-md-6">
                                                            <input type="text" id="TEUCapacity" class="form-control" data-bind="value: TEUCapacity, event: { keypress: $root.ValidateNumeric }, enable: $root.isClassificationSocitiesEnable" maxlength="15">
                                                            <span id="TEUCapacityValidateError" class="validationError"></span>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">No. of Bays:</label>
                                                            <div class="col-md-6">
                                                                <input type="text" id="NoOfBays" class="form-control" data-bind="value: NoOfBays, event: { keypress: $root.ValidateNumeric }, enable: $root.isNoOfBaysEnable" maxlength="2">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group1">
                                                            <label class="control-label col-md-6">No. of Rows on Deck:</label>
                                                            <div class="col-md-6">
                                                                <input type="text" id="NoOfRowsOnDesk" class="form-control" data-bind="value: NoOfRowsOnDesk, event: { keypress: $root.ValidateNumeric }, enable: $root.isNoOfRowsOnDeskEnable" maxlength="15">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">Official Number:</label>
                                                            <div class="col-md-6">
                                                                <input type="text" id="OfficialNumber" class="form-control" data-bind="value: OfficialNumber, event: { keypress: $root.ValidateNumeric }, enable: $root.isOfficialNumberEnable" maxlength="15">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">Vessel Build Year:<span class="required">*</span></label>
                                                            <div class="col-md-6">
                                                                <div class="form-control">
                                                                    <input type="text" id="VesselBuildYear" class="form-control datetime-picker" onkeydown="PreventBackSpace(event);" data-bind="kendoDatePicker: { value: VesselBuildYear, start: 'decade', depth: 'decade', max: new Date(), format: 'yyyy', enabled: $root.isVesselBuildYearEnable }, event: { keypress: $parent.ValidateDate }" maxlength="10" />
                                                                    <span class="validationError" data-bind="validationMessage: VesselBuildYear"></span>
                                                                </div>

                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">Port of Registry(POR):<span class="required">*</span></label>
                                                            <div class="col-md-6">
                                                                <div class="form-control">
                                                                    <input id="PortOfRegistry" data-bind="kendoAutoComplete: { data: $root.VesselReferenceDtl().ddlPOR, dataTextField: 'PortName', dataValueField: 'PortCode', value: PortOfRegistryName, select: $root.VesselSelect }, event: { change: ChangePortOfRegistry }" />
                                                                    <span class="validationError" id="spanPortOfRegistry" data-bind="validationMessage: PortOfRegistryName"></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>

                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">MMSI Number:<span class="required">*</span></label>
                                                            <div class="col-md-6">
                                                                <input type="text" id="MMSINumber" class="form-control" data-bind="value: MMSINumber, event: { keypress: $root.ValidateNumeric }, enable: $root.isMMSINumberEnable" maxlength="15">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group1">
                                                            <label class="control-label col-md-6">Ex Vessel Name:</label>
                                                            <div class="col-md-6">
                                                                <input type="text" id="ExVesselName" class="form-control" data-bind="value: ExVesselName, enable: $root.isExVesselNameEnable" maxlength="30">
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">Country/Nationality:<span class="required">*</span></label>
                                                            <div class="col-md-6">
                                                                <div class="form-control">
                                                                    <select name="documents" id="ddlVesselNationality" class="form-control" data-bind="options: $root.VesselReferenceDtl().ddlVesselNationality, optionsText: 'VNName', optionsValue: 'VNCode', value: VesselNationality, optionsCaption: 'Select...', enable: $root.isVesselNationalityEnable, event: { change: ChangeVesselNationality }">
                                                                    </select>
                                                                    <span class="validationError" id="spanVesselNationality" data-bind='validationMessage: VesselNationality'></span>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>

                                                </div>
                                                <div class="row">
                                                    <div class="col-md-4">
                                                        <div class="form-group">
                                                            <label class="control-label col-md-6">Is Govt. Vessel:</label>
                                                            <div class="col-md-6">
                                                                <div class="control-label" style="text-align: left;">
                                                                    <div class="radio-list">
                                                                        <div class="radio-list" id="divIsGovtVessel">
                                                                            <label class="radio-inline">
                                                                                <input id="IsGovtVessel1" type="radio" name="IsGovtVessel" value="Y" data-bind="checked: IsGovtVessel, enable: $root.isGovtVesselEnable">Yes</label>
                                                                            <label class="radio-inline">
                                                                                <input id="IsGovtVessel2" type="radio" name="IsGovtVessel" value="N" data-bind="checked: IsGovtVessel, enable: $root.isGovtVesselEnable">No</label>
                                                                        </div>
                                                                    </div>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group1">
                                                            <label class="control-label col-md-6">Status:</label>
                                                            <div class="col-md-6">
                                                                <div class="form-control">
                                                                    <select id="status" name="" data-bind="options: [{ name: 'Active', val: 'A' }, { name: 'Inactive', val: 'I' }], optionsText: 'name', optionsValue: 'val', value: RecordStatus, enable: $root.isClassificationSocitiesEnable, event: { change: $parent.CheckWfStatus } "></select>
                                                                </div>
                                                            </div>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <div class="form-group">

                                                            <div class="col-md-6">
                                                            </div>
                                                        </div>
                                                    </div>
                                                </div>
                                            </fieldset>
                                            <div class="clearfix">
                                                <br>
                                            </div>

                                            <div id="tableheader">
                                                <a class="button gbtngreen" data-bind="click: $root.AddEngineDet, visible: $root.isGridsEnable">Add New <i class="fa fa-plus"></i></a>
                                                <div class="search" style="display: none">
                                                    <select id="columns" onchange="sorter.search('query')"></select>
                                                    <input type="text" id="query" onkeyup="sorter.search('query')" placeholder="Search" />
                                                    <a class="button gbtn" href="javascript:sorter.reset()">Reset <i class="fa fa-rotate-left"></i></a>
                                                </div>
                                            </div>
                                            <div class="table-scrollable">
                                                <table cellpadding="0" cellspacing="0" border="0" id="tblQualities" class="tinytable">
                                                    <thead>
                                                        <tr>
                                                            <th>Engine Power (Kilo Watts)</th>
                                                            <th>Engine Type</th>
                                                            <th>Propulsion Type</th>
                                                            <th>No. of Propellers</th>
                                                            <th>Max. Speed (knots)</th>
                                                            <th>Action</th>
                                                        </tr>
                                                    </thead>

                                                    <tbody data-bind="foreach: VesselEngines">
