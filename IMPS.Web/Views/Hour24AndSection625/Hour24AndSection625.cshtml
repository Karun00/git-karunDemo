@{
    ViewBag.Title = "Hour 24 Report 62(5)";
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";   
}

@section scripts
{
    <script src="~/Scripts/Application/Model/Hour24AndSection625Model.js"></script>
    <script src="~/Scripts/Application/ViewModel/Hour24AndSection625ViewModel.js"></script>
    <script type="text/javascript">
        $(document).ready(function () {
            $("#CDContactNumber").kendoMaskedTextBox({
                mask: "(000)000-000-0000"
            });
            $("#CDMobileNumber").kendoMaskedTextBox({
                mask: "(000)000-000-0000"
            });
        });
    </script>
}
@section ko_apply
{     
var viewModel = new IPMSROOT.Hour24AndSection625ViewModel(true);
ko.applyBindingsWithValidation(viewModel, $("#Hour24AndSection625")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }});
}

<div class="row">
    <div class="col-md-12">
        <h3 class="page-title"><span id="spnTitle"></span></h3>
        <ul class="page-breadcrumb breadcrumb"></ul>
    </div>
</div>
<div class="clearfix"></div>
<script id="customMessageTemplate" type="text/html">
    <span class="validationError" data-bind='validationMessage: field'></span>
</script>

<div id="Hour24AndSection625">
    <div class="row" data-bind="loadingWhen: $root.viewModelHelper.isLoading">
        <div class="col-md-12">
            <div class="portlet" id="divFormWizardTabNavigation">
                <div class="portlet-body form">
                    <form action="#" class="form-horizontal" id="submit_form" data-bind="with: hour24andsection625Model ">
                        <div class="form-wizard" data-bind='validationOptions: { messageTemplate: "customMessageTemplate" }' id="divRegistration">
                            <div class="form-body">
                                <ul class="nav nav-pills nav-justified steps" id="ulTabs">
                                    <li>
                                        <a data-toggle="tab" class="step" data-bind="click: $root.GotoTab1" style="cursor: pointer">
                                            <span class="number">1
                                            </span>
                                            <span class="desc">
                                                <i class="fa fa-check"></i>24 Hour Report
                                            </span>
                                        </a>
                                    </li>
                                    <li>
                                        <a data-toggle="tab" class="step" data-bind="click: $root.GotoTab2" style="cursor: pointer">
                                            <span class="number">2
                                            </span>
                                            <span class="desc">
                                                <i class="fa fa-check"></i>Report Details
                                            </span>
                                        </a>
                                    </li>
                                </ul>
                                <div id="bar" class="progress progress-striped" role="progressbar">
                                    <div class="progress-bar progress-bar-success">
                                    </div>
                                </div>
                                <div class="tab-content">
                                    <!--Tab1 Starts-->
                                    <div class="tab-pane active" id="tab1" data-bind="visible: $parent.viewModeForTabs() == 'notification1', template: { name: 'notification1-template' }">
                                    </div>
                                    <!--Tab1 ends here-->
                                    <!--Tab2 Starts here-->
                                    <div class="tab-pane active" id="tab2" data-bind="visible: $parent.viewModeForTabs() == 'notification2', template: { name: 'notification2-template' }">
                                    </div>
                                    <!--Tab2 ends here-->
                                </div>
                            </div>
                            <div class="form-actions fluid">
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="btns-group">
                                            <a href="javascript:;" class="btn default button-previous" id="btnGoBack" title="Back" data-bind="click: $root.GoToPrevTab, visible: $parent.isGoBackVisible"><i class="m-icon-swapleft"></i> Back</a>
                                            <a href="#" class="btn default button-next" title="Next" data-bind="click: $root.ValidateForm, visible: $parent.isSaveVisible">Next <i class="m-icon-swapright"></i></a>
                                            <a href="#" class="btn green button-submit" title="Submit" data-bind="click: $root.SaveHour24reports, visible: $parent.isSubmitVisible">Submit <i class="m-icon-swapright m-icon-white"></i></a>
                                            <button type="submit" class="btn blue" title="Reset" data-bind="click: $parent.Resetform">Reset</button>
                                            <button type="button" class="btn red" title="Cancel" data-bind="click: $parent.Cancelform">Cancel</button>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
</div>

<script type="text/html" id="notification1-template">
    <div class="clearfix">
        <br>
    </div>
    <fieldset>
        <legend>24 Hour Report</legend>

        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label class="control-label col-md-5">Port:<span class="required">*</span> </label>
                    <div class="col-md-6">
                        <select class="form-control" data-bind="options: $root.hour24reportreferencedata().Ports, optionsText: 'PortName', optionsCaption: 'Choose....', optionsValue: 'PortCode', value: PortCode">    
                        </select>
                    </div>
                </div>
            </div>
        </div>
        <div class="row">
            <div class="col-md-6">
                <div class="form-group">
                    <label class="control-label col-md-5">Name of Operator:<span class="required">*</span> </label>
                    <div class="col-md-6">
                        <input type="text" id="OperatorName" class="form-control" data-bind=" value: OperatorName, event: { keypress: $parent.validationHelper.ValidateAlphaNumericWithSpaces  }" name="OperatorName" maxlength="30">
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label class="control-label col-md-4">License Number:<span class="required">*</span> </label>
                    <div class="col-md-6">
                        <input type="text" id="LincseNumber" class="form-control" data-bind=" value: LincseNumber" name="LincseNumber" maxlength="30">
                    </div>
                </div>
            </div>
        </div>
        <div class="clearfix"></div>
        <fieldset>
            <legend>Contact details of the person reporting the incident/Primary point of contact</legend>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label col-md-5">Name:<span class="required">*</span> </label>
                        <div class="col-md-6">
                            <input type="text" id="CDName" class="form-control" data-bind=" value: CDName, event: { keypress: $parent.validationHelper.ValidateAlphaNumericWithSpaces }" name="CDName" maxlength="30">
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label col-md-4">Designation:<span class="required">*</span> </label>
                        <div class="col-md-6">
                            <input type="text" id="CDDesignation" class="form-control" data-bind=" value: CDDesignation, event: { keypress: $parent.validationHelper.ValidateAlphaNumericWithSpaces }" name="CDDesignation" maxlength="30">
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label col-md-5">Contact Number:<span class="required">*</span> </label>
                        <div class="col-md-6">
                            <input type="text" id="CDContactNumber" class="form-control" onfocus="return ContactMasking();" onblur="return ContactMasking();" data-bind=" value: CDContactNumber, event: { keyup: HandleContactnumber, keypress: $parent.validationHelper.ValidateNumeric }" name="CDContactNumber" maxlength="13">
                            <span id="ContactNum" class="validationError"></span>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label col-md-4">Mobile Number:<span class="required">*</span> </label>
                        <div class="col-md-6">
                            <input type="text" id="CDMobileNumber" class="form-control" onfocus="return MobileMasking();" onblur="return MobileMasking();" data-bind=" value: CDMobileNumber, event: { keyup: HandleMobilenumber, keypress: $parent.validationHelper.ValidateNumeric }" name="CDMobileNumber " maxlength="13">
                            <span id="MobileNum" class="validationError"></span>
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label col-md-5">Email Id:<span class="required">*</span></label>
                        <div class="col-md-6">
                            <input type="text" id="CDEmailID" class="form-control" data-bind=" value: CDEmailID, event: { keypress: ValidateAlphanumeric }" name="CDEmailID" valueupdate:'keypress' maxlength="50">
                               <span id="spansectionDEmailID" class="validationErrorL"></span>
                        </div>
                    </div>
                </div>
                <div class="col-md-6"></div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label col-md-5">Address:<span class="required">*</span> </label>
                        <div class="col-md-6">
                            <textarea name="CDAddress" class="form-control" data-bind=" value: CDAddress" id="CDAddress" maxlength="500"></textarea>

                        </div>
                    </div>
                </div>
                <div class="col-md-6"></div>
            </div>
        </fieldset>
        <fieldset>
            <legend>Nature of Occurrence</legend>


            <div class="table-scrollable">
                <table cellpadding="0" cellspacing="0" border="0" id="tblmovementsdetails" class="tinytable">
                    <thead>
                        <tr>
                            <th><span class="required"></span></th>
                            <th>Nature <span class="required"></span></th>
                            <th><span class="required"></span></th>
                        </tr>
                    </thead>
                    <tbody data-bind="foreach: $root.hour24reportreferencedata().Naturetypes">
                        <tr>
                            <td>
                                <input type="radio" class="form-control" id="naturetype" name="Nature" data-bind=" Value: SubCatCode, checked: $root.hour24andsection625Model().NONatureCode, click: $parent.toggleAssociation">
                            </td>
                            <td>
                                <label data-bind=" text: SubCatName"></label>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
        </fieldset>
        <fieldset>
            <legend>Initial Occurrence Details</legend>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label col-md-5">Date & Time of Occurrence:<span class="required">*</span> </label>
                        <div class="col-md-6">
                            <div class="form-control">
                                <input id="IODOccuranceDateTime" onkeydown="PreventBackSpace();" class="form-control" onkeypress="return false;" data-bind="   kendoDateTimePicker: { value: IODOccuranceDateTime, format: 'yyyy-MM-dd HH:mm', timeFormat: 'HH:mm' }, event: { keypress: ValidateDate , change: $root.validationIODOccuranceDateTimeMainScreen }"  maxlength="10" />
                                 <span id="spanIODOccuranceDateTimemainscreen" class="validationErrorL"></span>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label col-md-4">Specific Location:<span class="required">*</span> </label>
                        <div class="col-md-6">
                            <input type="text" id="IODSpecificLocation" class="form-control" data-bind=" value: IODSpecificLocation, event: { keypress: $parent.validationHelper.ValidateAlphaNumeric }" name="IODSpecificLocation" maxlength="100">
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-6">
                    <div class="form-group">
                        <label class="control-label col-md-5">TimePeriod:<span class="required">*</span> </label>
                        <div class="col-md-6">
                            <input type="text" id="TimePeriod" class="form-control" name="TimePeriod" data-bind=" value: Timeperiod" maxlength="30">
                        </div>
                    </div>
                </div>
            </div>
            <div class="row">
                <div class="col-md-10">
                    <div class="form-group">
                        <label class="control-label col-md-3">Brief Description of Occurrence:<span class="required">*</span> </label>
                        <div class="col-md-9">
                            <textarea name="IODOccuranceBriefDescription" data-bind=" value: IODOccuranceBriefDescription" id="IODOccuranceBriefDescription" class="form-control" maxlength="500"></textarea>
                        </div>
                    </div>
                </div>
                <div class="col-md-6"></div>
            </div>
        </fieldset>
    </fieldset>
</script>

