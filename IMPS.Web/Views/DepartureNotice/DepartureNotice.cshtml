@model IPMS.Domain.ValueObjects.PrivilegeVO

@{
    ViewBag.Title = "Departure Notice";
    Layout = "~/Views/Shared/_IPMSLayout.cshtml";

    var DepartureID = string.Empty;
    var showViewOnLoad = false;
    if (!string.IsNullOrEmpty(ViewBag.DepartureID))
    {
        DepartureID = ViewBag.DepartureID;
        showViewOnLoad = true;
    }
}
@section scripts{
    <script src="~/Scripts/Application/ViewModel/WorkflowTaskExecution.js"></script>
    <script src="~/Scripts/Application/Model/DepartureNoticeModel.js"></script>
    <script src="~/Scripts/Application/ViewModel/DepartureNoticeViewModel.js"></script>
    <script src="~/Scripts/Lib/moment.js"></script>
}
@section ko_apply
{
    var showOnLoad = false;
    if ("@showViewOnLoad" == "True")
    {
         showOnLoad = true;
    }
    var viewModel = new IPMSROOT.DepartureNoticeViewModel(showOnLoad,'@DepartureID');	   	
	ko.applyBindingsWithValidation(viewModel, $("#divDepartureNotice")[0], { insertMessages: false, messagesOnModified: false, grouping: { deep: true }}   ); 
}

<!-- BEGIN CONTENT -->

<div class="row">
    <div class="col-md-12">
        <!-- BEGIN PAGE TITLE & BREADCRUMB-->
        <h3 class="page-title"><span id="spnTitle">Departure Notice</span></h3>
        <ul class="page-breadcrumb breadcrumb"></ul>
    </div>
</div>
<div id="divDepartureNotice">
    <div data-bind="visible: viewMode() == 'List', template: { name: 'DepartureNoticeList-template' }"></div>
    <div data-bind="visible: viewMode() == 'Form', template: { name: 'DepartureNoticeForm-template' }"></div>
    <div data-bind="visible: viewMode() == 'PopUp', template: { name: 'DepartureNoticePopUp-template' }"></div>
</div>

<!-- Main body content starts here for developers-->
<div class="clearfix"></div>


<!-- DepartureNotice List and Serach content starts here-->
<script type="text/html" id="DepartureNoticeList-template">
    <div class="portlet-body" data-bind="loadingWhen: $root.viewModelHelper.isLoading">
        <div class="table-toolbar">
             <div>
                <div class="portlet box advbg">
                    <div class="portlet-title">
                        <div class="caption">
                            <i class="fa fa-reorder"></i>Advanced Search
                        </div>
                        <div class="tools">
                            <a href="javascript:;" class="collapse"></a>
                        </div>
                    </div>
                    <div class="portlet-body">
                        <div class="form-wizard form-horizontal">
                            <div class="form-body">

                                <div class="form-group" data-bind="with: departureNoticeModel">
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="control-label col-md-6"> ETD From  :</label>
                                                <div class="col-md-6">
                                                    <div>
                                                        <input id="SubmissionDateFrom" name="SubmissionDateFrom" class="form-control" onkeypress="return false;" data-bind="kendoDatePicker: { value: SubmissionDateFrom, format: 'yyyy-MM-dd', readonly: true, month: { empty: '<span class=k-state-disabled>#= data.value #</span>' } }, event: { change: ValidDate }" maxlength="10" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="control-label col-md-6"> ETD To :</label>
                                                <div class="col-md-6">
                                                    <div>
                                                        <input id="SubmissionDateTO" name=" SubmissionDateTo" class="form-control" onkeypress="return false;" data-bind="kendoDatePicker: { value: SubmissionDateTO, format: 'yyyy-MM-dd', readonly: true, min: SubmissionDateFrom, month: { empty: '<span class=k-state-disabled>#= data.value #</span>' } }" maxlength="10" />
                                                    </div>
                                                </div>
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                
                                            </div>
                                        </div>
                                    </div>
                                    <div class="row">
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="control-label col-md-6">VCN :</label>
                                                <div class="col-md-6">
                                                    <div>
                                                        @*<input id="VCNSearch" name="VCN" class="form-control" data-bind="value: VCNSearch" />*@
                                                        
                                                        <input id="VCNName" name="VCNName"  onblur ="VCNonblur();" onkeydown="SerchVCNBackSpace();" onkeypress="return SerchVCNBackSpaceNumValid(event)" data-bind="kendoAutoComplete: {
    dataTextField: 'vcn', minLength: 3, pageSize: 1
                                                        , dataSource: {
                                                            serverFiltering: true,
                                                            transport: {
                                                                read: { url: '/api/DepartureNoticeVcnDetailsforAutocomplete', dataType: 'json' }
                                                            }, schema: {
                                                                data: function (data) { return data; }, total: function (data) {
                                                                    return data.length;
                                                                }
                                                            },
                                                        }, value: VCNSearch, select: $root.VCNSelectSearch
}" />
                                            <span id="spanVCNSearchValid" class="validationError" data-bind="visible: $root.isspanVCNSearchValid"></span>
                                      
                                                    </div>
                                                </div>
                                                </div>
                                            </div>
                                        

                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label class="control-label col-md-6">Vessel Name/IMO No. :</label>
                                                <div class="col-md-6">
                                                    <div>
                                                        @*<input id="VesselNameSearch" name="Vessel Name" class="form-control" data-bind="value: VesselNameSearch" />*@
                                                        
                                                  <input id="VesselName1" name="VesselName1"  onblur ="Vesselonblur();" onkeydown="SerchVesselBackSpace();"  @*onkeypress="SerchVesselKeyPress()"*@ data-bind="kendoAutoComplete: {
    dataTextField: 'VesselName', minLength: 3, pageSize: 1
                                                        , dataSource: {
                                                            serverFiltering: true,
                                                            transport: {
                                                                read: { url: '/api/DepartureNoticeVesselDetailsforAutocomplete/', data: { columnName: '' } }
                                                            }, schema: {
                                                                data: function (data) { return data; }, total: function (data) {
                                                                    return data.length;
                                                                }
                                                            },
                                                        }, value: VesselNameSearch, select: $root.VesselSelectSearch
}" />
                                                    <span id="spanVesselSearchValid" class="validationError" data-bind="visible: $root.isspanVesselSearchValid"></span>
                                               
                                                    </div>
                                                    </div>
                                                </div>
                                            </div>
                                       
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label class="control-label col-md-1"></label>
                                                <div class="col-md-8">
                                                    <div>
                                                        <button type="submit" class="btn btn-sm green" data-bind="click: $root.SrearchDepatureDet">Get Data</button>
                                                        <button type="submit" class="btn btn-sm blue" data-bind="click: $root.ResetSearchDet">Reset</button>
                                                    </div>
                                                </div>
                                            </div>
                                        </div>

                                    </div>

                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            <div  id="divDepartureNoticeList" data-bind="kendoGrid: {
    data: LoadDepartureNoticeDetails,
    groupable: false,
    sortable: true,
    scrollable: true,
    filterable: { extra: false, 
        operators: { 
            string: { 
                startswith: 'Starts with', 
                eq: 'Is equal to', 
                neq: 'Is not equal to' 
            },
            date: {
                gte: 'Is after or equal to',
                gt: 'Is after',
                lte: 'Is before or equal to',
                lt: 'Is before'
            }
        } 
    },
    rowTemplate: 'rowTmpl',
    pageable: { pageSize: 20, pageSizes: 20 },
    useKOTemplates: true,
    columns: [{ field: 'VCNSort', title: 'VCN', width: 80, filterable: true },
        { field: 'VesselNameSort', title: 'Vessel Name', width: 100, filterable: true },
        { field: 'VesselTypeSort', title: 'Vessel Type', width: 100, filterable: true },
        { field: 'SubmissionDateSort', title: 'ETD', filterable: true, width: 130, type: 'date' },
        { field: 'WFStatusSort', title: 'Status', filterable: true, width: 100 },
        { title: 'Actions', filterable: true, width: 100 }]
}">
            </div>
            <script id="rowTmpl" type="text/html">
                <tr>
                    <td>
                        <div class="nowrap"><span data-bind="text: VCN"></span></div>
                    </td>
                    <td>
                        <div class="nowrap"><span data-bind="text: VesselName"></span></div>
                    </td>
                    <td>
                        <div class="nowrap"><span data-bind="text: VesselType"></span></div>
                    </td>
                    <td>
                        <div class="nowrap"><span data-bind="text: SubmissionDate"></span></div>
                    </td>
                    <td>
                        <div class="nowrap"><span data-bind="text: WFStatust"></span></div>
                    </td>
                    <td>
                        <div class="nowrap">
                            @if (Model.HasViewPrivilege)
                            {
                                <a href="#" data-bind="click: $root.viewDepartureNotice, visible: isViewVisible" title="View"><i class="fa fa-eye"></i></a>
                            }
                            @if (Model.HasEditPrivilege)
                            {
                                <a href="#" data-bind="click: $root.acknowledgeDepartureNotice, visible: isAcknowledgeVisible" title="Submit"><i class="fa fa-save"></i></a>                                                           
                                <a href="#" data-bind="click: $root.editDepartureNotice, visible: isEditVisible" title="Edit"><i class="fa fa-edit"></i></a>
                            }
                            <a href="#" data-bind="click: $parent.viewWorkFlow" title="View Workflow"><i class="fa fa-stack-exchange"></i></a>
                        </div>
                    </td>
                </tr>
            </script>
        </div>
    </div>
   <div class="form-body" data-bind="with: departureNoticeModel">
                <div class="form-group">
                    <div id="stack1" class="modal fade" tabindex="-1" data-width="400">
                        <div class="modal-dialog" style="width: 500px;">
                            <div class="modal-content">
                                <div class="modal-body">
                                    <button type="button" class="close" data-dismiss="modal" aria-hidden="true"></button>
                                    <div class="scroller" style="height: 225px" data-always-visible="1" data-rail-visible1="1">
                                        <fieldset>
                                            <legend id="spnTitile1">WorkFlow Remarks</legend>
                                           <div align="center"><textarea rows="3" cols="50" class="form-control"  id="WorkFlowRemarks"  style="width: 350px; height: 180px; text-align: center; margin:  0px 9.5px 0px 0px;resize: none;" data-bind="value: WorkFlowRemarks" disabled=""></textarea></div>
                                       </fieldset>
                                    </div>
                                </div>
                                <br>
                                &nbsp;
                            </div>
                        </div>
                    </div>
                </div>
            </div>
    
</script>

<script id="customMessageTemplate" type="text/html" data-bind="loadingWhen: $root.viewModelHelper.isLoading">
    <span class="validationError" data-bind="validationMessage: field"></span>
</script>

<!-- DepartureNotice form for Add Edit and View starts here-->
<script type="text/html" id="DepartureNoticeForm-template">
