                                        <div class="col-md-6">
                                            <input type="text" class="form-control" data-bind="value: ArrDraft, enable: $root.IsEditableView">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label class="control-label col-md-6">Dep. Draft (M):</label>
                                        <div class="col-md-6">
                                            <input type="text" class="form-control" data-bind="value: DepDraft, enable: $root.IsEditableView">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-md-4">
                                    <div class="form-group">
                                        <label class="control-label col-md-6">Next Port of Call:</label>
